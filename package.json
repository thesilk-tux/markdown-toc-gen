{
  "name": "markdown-toc-gen",
  "version": "0.1.0",
  "description": "Generating and updating table of contents in Markdown files which conform with prettier.",
  "bin": {
    "markdown-toc-gen": "bin/index.js"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier:check": "prettier -c .",
    "prettier:write": "prettier -w .",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "toc",
    "markdown",
    "generator"
  ],
  "files": [
    "bin",
    "RELEASE_NOTES.md"
  ],
  "author": "TheSilk <thesilk (at) posteo.net>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.27",
    "@types/yargs": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "inversify": "^5.0.5",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.4"
  },
  "dependencies": {
    "yargs": "^16.2.0"
  }
}
